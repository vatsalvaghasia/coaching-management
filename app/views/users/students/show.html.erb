<div class="container float-right">
	<h1>Student Details</h1>
<table class="table table-bordered table-striped">
	<thead class="thead-dark">
		<th>Student Id</th>
		<th>Student Email</th>
		<th>Name</th>
		<th>Phone</th>
	</thead>
	<tbody>
		<tr>
			<td><%= @student.id %></td>
			<td><%= @student.email %></td>
			<td><%= @student.email %></td>
			<td><%= @student.phone %></td>
		</tr>
	</tbody>
</table>
</div>
